  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "a60daf9a",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-07-25T01:41:47.711163Z",
     "iopub.status.busy": "2024-07-25T01:41:47.710738Z",
     "iopub.status.idle": "2024-07-25T01:41:47.729809Z",
